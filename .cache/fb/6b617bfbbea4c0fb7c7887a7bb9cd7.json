 width: 100%;\n  max-width: 100%;\n  z-index: -1;\n}\n\n.container {\n  max-width: 46em;\n  margin: 0 auto;\n  padding-left: 1em;\n  padding-right: 1em;\n  padding-bottom: 1.5em;\n}\n\n.screen-reader {\n  display: none;\n}\n\nheader {\n  display: grid;\n  grid-template-columns: 1fr;\n  grid-template-rows: 1fr;\n  place-items: center;\n}\n\n.grid {\n  display: grid;\n  grid-template-columns: 1fr;\n  grid-template-rows: 1fr;\n  overflow: hidden;\n}\n\n.angle {\n  width: 100%;\n  height: 10vw;\n  grid-row: 1;\n  grid-column: 1;\n}\n\n.start {\n  align-self: start;\n  margin-top: -1px; /* hack to solve responsive sizing was ocassionally showing a single line of color below the text */\n}\n\n.end {\n  align-self: end;\n  margin-bottom: -1px; /* hack to solve responsive sizing was ocassionally showing a single line of color below the text */\n}\n\n.five-em {\n  height: 5em; /* this matches the biggest the io can push the translate */\n}\n\n.margin-five {\n  margin-top: 4.9em; // has the same hack as start\n}\n\n.gcr-1 {\n  grid-column: 1;\n  grid-row: 1;\n}\n\nheader picture {\n  min-height: 20em; // hack to fix small size the logo getting hidden\n}\n\n.header-logo {\n  padding: $global-padding;\n  max-width: 50%;\n}\n\n.io-push {\n  animation-timing-function: ease; // needed to stop the page jumping on a part of the io pulling up and down\n  // animation-name: slidein;\n}\n\n// this isnt working and needs to be used on the margins\n/* @keyframes slidein {\n  from {\n  opacity: 0;\n  margin-top: 0;\n  }\n\n  to {\n  opacity: 100%;\n  margin-top: 20em;\n  }\n} */\n\n.io-parallax {\n  animation-timing-function: ease;\n}\n\n#services {\n  display: grid;\n  grid-gap: 2em;\n  // https://codepen.io/rachelandrew/pen/GZQYQa\n  grid-template-columns: repeat(auto-fill, minmax(22em,1fr));\n  // has a minor problem when the screen gets below 22em wide\n}\n/*\n#services {\n      @media (max-width: 22em) {\n      width: 50%;\n    }\n}\n*/\n\n#services article {\n    @media (max-width: 